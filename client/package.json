		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@types/jest": "^26.0.23",
		"@types/jsonwebtoken": "^8.5.4",
		"@types/node": "^15.6.0",
