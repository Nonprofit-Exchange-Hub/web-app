{
	"name": "client",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@emotion/react": "^11.9.0",
		"@emotion/styled": "^11.8.1",
		"@mui/icons-material": "^5.6.2",
		"@mui/material": "^5.7.0",
		"@mui/styles": "^5.7.0",
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
