{
    "name": "neh-backend",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write './{src,test}/**/*.ts' --config ../.prettierrc",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:dev:db": "sudo docker-compose up",
        "start:prod": "node dist/main",
        "lint": "eslint src/**/*.ts",
        "test": "jest --config ./src/jest.config.json",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
        "checks": "npm run test:e2e && npm run test && npm run lint",
        "prepare": "cd .. && husky install && cd server",
        "seed:drop": "ts-node -r tsconfig-paths/register src/database/seeding/seed.ts"
    },
    "dependencies": {
        "@nestjs/common": "^8.4.7",
        "@nestjs/config": "^2.1.0",
        "@nestjs/core": "^8.4.7",
        "@nestjs/jwt": "^8.0.1",
        "@nestjs/mapped-types": "^1.0.1",
        "@nestjs/passport": "^8.2.2",
        "@nestjs/platform-express": "^8.4.7",
        "@nestjs/platform-socket.io": "^8.4.7",
        "@nestjs/serve-static": "^2.2.2",
        "@nestjs/typeorm": "^8.1.4",
        "@nestjs/websockets": "^8.4.7",
        "@sendgrid/mail": "^7.7.0",
        "@types/cookie-parser": "^1.4.2",
        "bcrypt": "^5.0.1",
        "bcryptjs": "^2.4.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "config": "^3.3.6",
        "cookie-parser": "^1.4.6",
        "dotenv": "^10.0.0",
        "faker": "^6.6.6",
        "hbs": "^4.1.2",
        "node-fetch": "^2.6.7",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pg": "^8.7.1",
        "pg-promise": "^10.9.5",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.5.6",
        "save": "^2.4.0",
        "typeorm": "^0.3.6"
    },
    "devDependencies": {
        "@faker-js/faker": "^7.3.0",
        "@nestjs/cli": "^8.2.8",
        "@nestjs/schematics": "^8.0.11",
        "@nestjs/testing": "^8.4.7",
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.11",
        "@types/faker": "^6.6.9",
        "@types/jest": "^26.0.24",
        "@types/node": "^14.14.31",
        "@types/node-fetch": "^3.0.3",
        "@types/passport-jwt": "^3.0.5",
        "@types/passport-local": "^1.0.33",
        "@types/supertest": "^2.0.10",
        "@typescript-eslint/eslint-plugin": "^4.15.2",
        "@typescript-eslint/parser": "^4.15.2",
        "eslint": "^7.20.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "husky": "^7.0.4",
        "jest": "26.6.0",
        "lint-staged": "^12.1.4",
        "prettier": "^2.5.1",
        "supertest": "^6.1.6",
        "ts-jest": "^26.5.6",
        "ts-loader": "^8.0.17",
        "ts-node": "^10.7.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.1.5",
        "webpack": "^5.51.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "./",
        "testRegex": [
            ".*\\.spec\\.ts$",
            ".*\\.e2e-spec\\.ts$"
        ],
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "lint-staged": {
        "./src/**/*.ts": [
            "eslint --fix"
        ]
    }
}
